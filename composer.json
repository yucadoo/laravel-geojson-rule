{
    "name": "yucadoo/laravel-geojson-rule",
    "type": "library",
    "description": "Laravel Validation Rule for GeoJSON.",
    "keywords": [
        "yuca",
        "laravel",
        "geojson",
        "rule"
    ],
    "homepage": "https://github.com/yucadoo/laravel-geojson-rule",
    "license": "MIT",
    "authors": [
        {
            "name": "Hrvoje Jukic",
            "email": "hrcajuka@gmail.com",
            "homepage": "https://github.com/yucadoo",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/contracts": "^5.5|^6.0|^7.0|^8.0|^9.0|^10|^11.0|^12.0",
        "jmikola/geojson": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.5.7"
    },
    "autoload": {
        "psr-4": {
            "YucaDoo\\LaravelGeoJsonRule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YucaDoo\\LaravelGeoJsonRule\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "thanks": {
            "name": "jmikola/geojson",
            "url": "https://github.com/jmikola/geojson"
        }
    },
    "config": {
        "sort-packages": true
    }
}
